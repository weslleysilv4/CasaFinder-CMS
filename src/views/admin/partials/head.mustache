    <link rel="stylesheet" href="styles/sidebars.css">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script>
      $(document).ready(function() {
      // Captura o evento de clique nos links da barra lateral
      $('.nav-link').click(function() {
      // Remove a classe 'active' de todos os links da barra lateral
      $('.nav-link').removeClass('active');
      // Adiciona a classe 'active' apenas ao link clicado
      $(this).addClass('active');
      });
      console.log("teste")
    });
    </script>
