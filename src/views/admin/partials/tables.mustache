  <table class="table table-striped table-hover mt-4 text-center align-middle">
  <thead class="table-dark">
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Imagem</th>
      <th scope="col">Nome</th>
      <th scope="col">Valor</th>
      <th scope="col">Endereço</th>
      <th scope="col">Criado Por</th>
      <th scope="col" colspan="2">Ação</th>
    </tr>
  </thead>
  <tbody class="data">
    {{#data}}
    <tr>
      <th scope="row">{{id}}</th>
      <td><img src="{{imgURL}}" alt="logo" style="max-width: 40px;"></td>
      <td>{{title}}</td>
      <td>{{price}}</td>
      <td>{{address}}</td>
      <td>{{createdBy}}</td>
      <td>
        <button type="button" class="btn btn-primary edit-btn" data-id="{{id}}" data-bs-toggle="modal" data-bs-target="#modalEdit">Editar</button>
      </td>
      <td>
        <button type="button" class="btn btn-danger edit-btn" data-id="{{id}}" data-bs-toggle="modal" data-bs-target="#modalRemove">Excluir</button>
      </td>
    </tr>
    {{/data}}
  </tbody>
  </table>
  {{> admin/partials/modalEdit}}
